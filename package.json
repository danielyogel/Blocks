{
  "name": "@daniely/knowledge-editor",
  "description": "Knowledge Editor",
  "author": "Daniel Yogel",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently  --group  --raw \"vite\" \"tsc --noEmit --watch\" \"npm run storybook\" \"npm run test\"",
    "build": "concurrently --raw \"vite build\" \"npm run build:types\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "peerDependencies": {
    "mobx": "^6.5.0",
    "react": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "classnames": "^2.3.1",
    "concurrently": "7.0.0",
    "postcss": "^8.4.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "storybook-builder-vite": "^0.1.21",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.3.1",
    "type-fest": "2.12.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.7.12"
  },
  "dependencies": {
    "fp-ts": "2.11.9",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "nanoid": "^3.3.1"
  }
}
